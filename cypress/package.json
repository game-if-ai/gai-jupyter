{
  "name": "gameifai-cypress",
  "description": "cypress testing",
  "version": "1.0.0",
  "dependencies": {
    "@simonsmith/cypress-image-snapshot": "^8.1.1",
    "cypress": "^12.17.4",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "cy:open": "npx cypress open --env failOnSnapshotDiff=false",
    "cy:open:withSnapshots": "npx cypress open",
    "cy:run": "npx cypress run --env failOnSnapshotDiff=false",
    "cy:run:withSnapshots": "npx cypress run",
    "cy:verify": "npx cypress verify"
  }
}
